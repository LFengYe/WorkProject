                android:layout_marginLeft="@dimen/reserve_text_left"
                />
            <TextView
                android:id="@+id/reserve_verify_text2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="@dimen/reserve_verify_text"
                android:textColor ="@color/text_black"
                />
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/reserve_linear_height"
            android:gravity="center_vertical"
            android:orientation="horizontal"
            >
            <TextView
                android:layout_width="@dimen/reserve_verify_text_weight"
                android:layout_height="wrap_content"
                android:text="@string/reserve_verify_text4"
                android:textSize="@dimen/reserve_verify_text"
