                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="@dimen/reserve_verify_text"
                android:textColor ="@color/text_black"
                />
        </LinearLayout>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/my_add_line_height"
            android:layout_marginTop="15dp"
            android:background="@color/white"
            >
            <Button
                android:id="@+id/reserve_verify_button"
                android:layout_width="@dimen/login_layout_width"
                android:layout_height="@dimen/login_layout_height"
                android:background="@drawable/login_layout"
                android:text="@string/reserve_verify_text8"
                android:textColor="@color/text_black"
                android:layout_centerInParent="true"
                android:layout_marginTop="5dp"

                />
        </RelativeLayout>



    </LinearLayout>

